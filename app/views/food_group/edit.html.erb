<%= render "partials/alert" %>
<%= form_for @food_group, url: food_group_path(@food_group.id), method: :patch do |f| %>
	<%= render 'food_group_form', grouped_foods: @grouped_foods, f: f, food_group_pairings: [], disabled: @disabled %>
	<%= render 'selected_food_table' %>

	<%= f.submit "Update Food Pairings",
		class: 'btn boomslang-orange-background pull-right',
		disabled: @disabled
		%>
<% end %>
